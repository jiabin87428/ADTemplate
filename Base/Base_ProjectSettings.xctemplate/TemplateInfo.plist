<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Kind</key>
    <string>Xcode.Xcode3.ProjectTemplateUnitKind</string>
    <key>Identifier</key>
    <string>com.apple.dt.unit.base_ProjectSettingsLee</string>
    <!-- This template defines the basic build settings that should be shared by almost every project. It defines project-level settings as well as target level settings. But the target settings are decalred non-concretely so inheritors will not create a target unless they specify their own concrete target definition. This temnplate also includes some Swift-specific prject and target settings that apply when the languageOption option is set to Swift. -->
    <key>Project</key>
    <dict>
        <!-- All these project-level settings are basically settings that we wished were the defaults... but they aren't -->
        <key>SharedSettings</key>
        <dict>
            <key>GCC_C_LANGUAGE_STANDARD</key>
            <string>gnu99</string>
            <key>CLANG_CXX_LANGUAGE_STANDARD</key>
            <string>gnu++0x</string>
            <key>CLANG_CXX_LIBRARY</key>
            <string>libc++</string>
            <key>GCC_WARN_ABOUT_RETURN_TYPE</key>
            <string>YES_ERROR</string>
            <key>GCC_WARN_UNINITIALIZED_AUTOS</key>
            <string>YES_AGGRESSIVE</string>
            <key>CLANG_WARN_UNREACHABLE_CODE</key>
            <string>YES</string>
            <key>GCC_WARN_UNUSED_VARIABLE</key>
            <string>YES</string>
            <key>GCC_WARN_UNUSED_FUNCTION</key>
            <string>YES</string>
            <key>CLANG_WARN__DUPLICATE_METHOD_MATCH</key>
            <string>YES</string>
            <key>CLANG_WARN_EMPTY_BODY</key>
            <string>YES</string>
            <key>CLANG_WARN_CONSTANT_CONVERSION</key>
            <string>YES</string>
            <key>CLANG_WARN_INT_CONVERSION</key>
            <string>YES</string>
            <key>CLANG_WARN_BOOL_CONVERSION</key>
            <string>YES</string>
            <key>CLANG_WARN_DOCUMENTATION_COMMENTS</key>
            <string>YES</string>
            <key>CLANG_WARN_ENUM_CONVERSION</key>
            <string>YES</string>
            <key>CLANG_WARN_OBJC_ROOT_CLASS</key>
            <string>YES_ERROR</string>
            <key>CLANG_ANALYZER_NONNULL</key>
            <string>YES</string>
            <key>CLANG_ANALYZER_NUMBER_OBJECT_CONVERSION</key>
            <string>YES_AGGRESSIVE</string>
            <key>GCC_WARN_UNDECLARED_SELECTOR</key>
            <string>YES</string>
            <key>GCC_WARN_64_TO_32_BIT_CONVERSION</key>
            <string>YES</string>
            <key>CLANG_WARN_DIRECT_OBJC_ISA_USAGE</key>
            <string>YES_ERROR</string>
            <key>CLANG_WARN_INFINITE_RECURSION</key>
            <string>YES</string>
            <key>CLANG_WARN_SUSPICIOUS_MOVE</key>
            <string>YES</string>
            <key>CLANG_ENABLE_OBJC_ARC</key>
            <string>YES</string>
            <key>CLANG_ENABLE_MODULES</key>
            <string>YES</string>
            <key>GCC_NO_COMMON_BLOCKS</key>
            <string>YES</string>
            <key>ENABLE_STRICT_OBJC_MSGSEND</key>
            <string>YES</string>
            <key>ALWAYS_SEARCH_USER_PATHS</key>
            <string>NO</string>
            <key>COPY_PHASE_STRIP</key>
            <string>NO</string>
        </dict>
        <key>Configurations</key>
        <dict>
            <key>Debug</key>
            <dict>
                <key>ONLY_ACTIVE_ARCH</key>
                <string>YES</string>
                <key>GCC_OPTIMIZATION_LEVEL</key>
                <string>0</string>
                <key>GCC_PREPROCESSOR_DEFINITIONS</key>
                <string>DEBUG=1 $(inherited)</string>
                <key>MTL_ENABLE_DEBUG_INFO</key>
                <string>YES</string>
                <key>ENABLE_TESTABILITY</key>
                <string>YES</string>
                
                <!-- !!!:20150416 Is it really appropriate for this to be different for Debug 
                 -->
                <key>GCC_DYNAMIC_NO_PIC</key>
                <string>NO</string>
                <key>DEBUG_INFORMATION_FORMAT</key>
                <string>dwarf</string>
            </dict>
            <key>Release</key>
            <dict>
                <key>ENABLE_NS_ASSERTIONS</key>
                <string>NO</string>
                <key>MTL_ENABLE_DEBUG_INFO</key>
                <string>NO</string>
                <key>DEBUG_INFORMATION_FORMAT</key>
                <string>dwarf-with-dsym</string>
            </dict>
            <key>AdHoc</key>
            <dict>
                <key>ENABLE_NS_ASSERTIONS</key>
                <string>NO</string>
                <key>MTL_ENABLE_DEBUG_INFO</key>
                <string>NO</string>
                <key>DEBUG_INFORMATION_FORMAT</key>
                <string>dwarf-with-dsym</string>
            </dict>
        </dict>
    </dict>
    <key>Targets</key>
    <array>
        <dict>
            <!-- Declare basic stuff that is true of all targets, but mark this non-concrete so it won't cause actual creation of a target. -->
            <key>Concrete</key>
            <false/>
            <key>Name</key>
            <string>___PACKAGENAME___</string>
            <key>SharedSettings</key>
            <dict>
                <key>PRODUCT_NAME</key>
                <string>$(TARGET_NAME)</string>
            </dict>
            <key>Configurations</key>
            <dict>
                <key>Debug</key>
                <dict/>
                <key>Release</key>
                <dict/>
                <key>AdHoc</key>
                <dict/>
            </dict>
        </dict>
    </array>
    <key>Options</key>
    <array>
        <dict>
            <key>Identifier</key>
            <string>languageChoice</string>
            <key>Units</key>
            <dict>
                <key>Swift</key>
                <dict>
                    <key>Project</key>
                    <dict>
                        <key>Configurations</key>
                        <dict>
                            <key>Debug</key>
                            <dict>
                                <key>SWIFT_OPTIMIZATION_LEVEL</key>
                                <string>-Onone</string>
                                <key>SWIFT_ACTIVE_COMPILATION_CONDITIONS</key>
                                <string>DEBUG</string>
                            </dict>
                            <key>Release</key>
                            <dict>
                                <key>SWIFT_OPTIMIZATION_LEVEL</key>
                                <string>-Owholemodule</string>
                            </dict>
                        </dict>
                    </dict>
                    <key>Targets</key>
                    <array>
                        <dict>
                            <!-- Declare basic stuff that is true of all Swift targets, but mark this non-concrete so it won't cause actual creation of a target. -->
                            <key>Concrete</key>
                            <false/>
                            <key>SharedSettings</key>
                            <dict>
                                <key>SWIFT_VERSION</key>
                                <string>___DEFAULTTOOLCHAINSWIFTVERSION___</string>
                            </dict>
                        </dict>
                    </array>
                </dict>
            </dict>
        </dict>
    </array>
</dict>
</plist>
